{
    "name": "fastify-learn",
    "version": "1.0.0",
    "main": "server.js",
    "license": "MIT",
    "dependencies": {
        "@fastify/swagger": "^8.12.0",
        "@fastify/swagger-ui": "^1.10.1",
        "@prisma/client": "^5.5.2",
        "@types/mongoose": "^5.11.97",
        "dotenv": "^16.3.1",
        "fastify": "^4.24.3",
        "module-alias": "^2.2.3",
        "pino-pretty": "^10.2.3"
    },
    "devDependencies": {
        "@types/node": "^20.8.10",
        "nodemon": "^3.0.1",
        "prisma": "^5.5.2",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "prisma": "npx prisma generate --schema=./prisma/postgres.prisma && npx prisma migrate dev --schema=./prisma/postgres.prisma",
        "build": "rm -rf ./dist && tsc",
        "start": "cp .env.production .env && NODE_ENV=production && node -r tsconfig-paths/register dist/app.js",
        "dev": "cp .env.development .env && NODE_ENV=development && nodemon -r tsconfig-paths/register src/app.ts"
    },
    "_moduleAliases": {
        "@src": "dist",
        "@constants": "dist/constants",
        "@utils": "dist/utils",
        "@interfaces": "dist/interfaces"
    }
}
